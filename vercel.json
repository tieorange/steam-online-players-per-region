{
    "version": 2,
    "builds": [
        {
            "src": "api/proxy.js",
            "use": "@vercel/node"
        },
        {
            "src": "build/web/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/proxy",
            "dest": "/api/proxy.js"
        },
        {
            "src": "/(.*)",
            "dest": "/build/web/$1"
        }
    ]
}