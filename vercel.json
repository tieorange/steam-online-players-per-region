{
    "version": 2,
    "builds": [
        {
            "src": "api/proxy.js",
            "use": "@vercel/node"
        },
        {
            "src": "build/web/**",
            "use": "@vercel/static"
        }
    ],
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        },
        {
            "source": "/(.*).js",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "routes": [
        {
            "src": "/api/proxy",
            "dest": "/api/proxy.js"
        },
        {
            "src": "/(.*)",
            "dest": "/build/web/$1"
        }
    ]
}